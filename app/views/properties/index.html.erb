<%= render 'new' %>
<%= render 'find' %>

<div id="map"
     style="width: 100%; height: 400px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
</div>



  <% @properties.each do |prop| %>
  <div>
    <%= link_to prop.address, property_path(prop) %> |
    <%= prop.latitude %> |
    <%= prop.longitude %>
  </div>
  <% end %>
  
